  },
  "dependencies": {
    "@hapi/hapi": "^21.3.12",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "amqplib": "^0.10.4",
    "auto-bind": "^4.0.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "nanoid": "^3.3.8",
    "pg": "^8.12.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "node-pg-migrate": "^7.9.1"
  }
}
